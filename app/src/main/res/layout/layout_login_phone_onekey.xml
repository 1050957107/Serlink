<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/cl_login_onekey"
    android:visibility="visible"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/tv_login_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="153****4769"
        android:textColor="@color/color_333333"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_login_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:text="@string/register"
        android:textColor="@color/color_mian"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_login_phone"
        app:layout_constraintLeft_toRightOf="@+id/tv_login_phone"
        app:layout_constraintTop_toTopOf="@+id/tv_login_phone"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_login_protective_shield"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:drawableLeft="@drawable/icon_login_protective_shield"
        android:drawablePadding="5dp"
        android:text="@string/protective_shield"
        android:textColor="@color/color_b6b6b6"
        android:textSize="11sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_login_phone"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
